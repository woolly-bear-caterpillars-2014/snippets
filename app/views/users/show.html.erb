<% if session[:user_id] %>

  <div id="wrapper">
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#<%= @tags.first %>">
                        &#9651; TOP
                    </a>
                </li>

                <% @tags.each do |tag| %>
                  <li>
                      <a href="#<%= tag %>"><%= tag.upcase %></a>
                  </li>
              <% end %>
            </ul>
        </div>

        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <%= render 'cheatsheets/show' %>

<% else %>

    <%= render 'users/new' %>

<% end %>



