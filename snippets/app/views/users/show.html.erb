<% if session[:user_id] %>

  <%= link_to "Log Out", users_logout_path, method: :delete %>
  <h1><%= @user.email %></h1>

  <%= link_to "Edit Account", edit_user_path %>
  <%= link_to "Delete Account", user_path(@user.id), method: :delete %>

  <%= link_to "Create a New Cheatsheet" %>

  <%= render 'cheatsheets/show' %>

<% else %>

  <%= render 'form' %>

<% end %>



